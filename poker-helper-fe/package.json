{
  "name": "poker-helper-react-ver",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "fetch-mock": "^7.5.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.1",
    "react-test-renderer": "^16.10.2",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
